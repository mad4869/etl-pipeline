generator client {
    provider             = "prisma-client-py"
    recursive_type_depth = "5"
}

datasource db {
    provider = "postgresql"
    url      = "postgresql://postgres:password123@localhost:5433/etl_db"
}

model amazon_sales_data {
    id             Int     @id @default(autoincrement())
    name           String? @db.VarChar
    main_category  String? @db.VarChar
    sub_category   String? @db.VarChar
    image          String? @db.VarChar
    link           String? @db.VarChar
    ratings        String? @db.VarChar
    no_of_ratings  String? @db.VarChar
    discount_price String? @db.VarChar
    actual_price   String? @db.VarChar
    Unnamed__0     String? @map("Unnamed: 0") @db.VarChar
}
